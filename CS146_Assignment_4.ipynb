      "source": [
        "## Generate new data by sampling from the posterior distribution\n",
        "Calculate the test statistic for the replicated data (min value)"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "x7gNz06CYR3t",
        "colab_type": "code",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 71
        },
        "outputId": "c3ab4ce3-2e69-46b4-c77c-3c28044e637e"
      },
      "source": [
        "n_sample = 10000\n",
        "control_sample = np.random.choice(posterior_samples_control['p'], size= n_sample) # sample p from the posterior distribution\n",
        "num_patients = [15, 18, 10, 39, 29, 10] # number of patients in each medical trial\n",
        "\n",
        "replicated_stat = []\n",
        "for i in range(0, n_sample):\n",
        "  replicated_data = []\n",
        "  for t in range(len(num_patients)):\n",
        "    replicated_array = sts.binom.rvs(num_patients[t], control_sample[i]) # draw number of improved patients for every trial from a binomial distribution\n",
        "    replicated_data.append(replicated_array) # putting together list of improved patients for each p\n",
        "  replicated_stat.append(np.amin(replicated_data)) # calculating the test statistic for every list and appending\n",
        "\n",
        "print(replicated_stat)\n",
        "print(np.mean(replicated_stat))"
      ],
      "execution_count": 86,
    },
